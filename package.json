{
  "version": "0.0.1",
  "type": "module",
  "name": "cl-lib",
  "files": [
    "dist"
  ],
  "main": "./dist/cl-lib.umd.js",
  "module": "./dist/cl-lib.es.js",
  "exports": {
    ".": {
      "import": "./dist/cl-lib.es.js",
      "require": "./dist/cl-lib.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:types",
    "build:types": "vue-tsc --project tsconfig.build-types.json --declaration --emitDeclarationOnly --outDir dist/types ",
    "typecheck": "vue-tsc --project tsconfig.build-types.json --noEmit",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@sentry/tracing": "^7.24.2",
    "@sentry/vue": "^7.24.2",
    "@headlessui/tailwindcss": "^0.1.2",
    "@headlessui/vue": "^1.7.7",
    "@heroicons/vue": "^2.0.13",
    "@jonyak/icons": "^1.0.6", 
    "vue": "^3.2.45",
    "vue-code-highlight": "^0.7.8",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/types": "^7.20.7",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.2.6",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.15",
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "^18.11.18",
    "@types/vue-router": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.8",
    "eslint-plugin-vue": "^9.8.0",
    "jsdom": "^20.0.3",
    "postcss": "^8.4.19",
    "prettier": "2.8.1",
    "sass": "^1.57.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-dts": "^1.7.1",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.25.8",
    "vue": "^3.2.45",
    "vue-eslint-parser": "^9.1.0",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^1.0.19"
  }
}
